#include <bits/stdc++.h>
using namespace std;
int main() {
	// your code goes here
int t,n,m;
cin>>t;
while(t--){
cin>>n>>m;
int a[n],b[m];
for(int i=0;i<n;i++)
  cin>>a[i];
for(int j=0;j<m;j++)
  cin>>b[j];
int flag=-1,c=-1,res;
for(int i=0;i<n;i++){
 for(int j=0;j<m;j++){
  if(a[i]==b[j]){
   flag=0;
    break;
  }}
 if(flag==0){
  res=a[i];c=0;
  break;}
}
 if(c==0){
  cout<<"YES"<<endl;
  cout<<1<<" "<<res<<endl;}
 else
  cout<<"NO"<<endl;
  }
	return 0;
}
